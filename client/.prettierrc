{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "jsxBracketSameLine": false,
  "importOrder": [
    "^react$",
    "^react-dom$",
    "^prop-types$",
    "^@mui/(.*)$",
    "^@reduxjs/toolkit$",
    "^react-redux$",
    "^redux(.*)$",
    "^@react-query/(.*)$",
    "^axios$",
    "^lodash(.*)$",
    "^@fortawesome/(.*)$",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true
}
